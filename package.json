{
  "name": "thinkorm",
  "version": "0.0.1",
  "description": "Generate SQL queries.",
  "main": "index.js",
  "scripts": {
    "test": "npm run eslint && npm run test-cov",
    "test-cov": "istanbul cover ./node_modules/mocha/bin/_mocha -- -t 50000 --recursive  -R spec test/",
    "compile": "babel src -d lib",
    "watch-compile": "npm run compile -- --watch",
    "prepublish": "npm run compile && npm test",
    "eslint": "eslint src/",
    "pack": "npm shrinkwrap && shrinkpack ."
  },
  "babel": {
    "presets": [
      "es2015-loose",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richenlin/thinkorm.git"
  },
  "author": "richenlin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/richenlin/thinkorm/issues"
  },
  "homepage": "https://github.com/richenlin/thinkorm#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-2": "^6.11.0",
    "eslint": "3.1.1",
    "istanbul": "0.4.4",
    "mocha": "2.5.3",
    "muk": "0.5.2"
  },
  "dependencies": {
    "babel-runtime": "6.9.2",
    "bluebird": "3.4.1",
    "knex": "^0.11.9",
    "lodash": "^4.14.0",
    "sprintf-js": "^1.0.3",
    "mysql2": "^1.0.0-rc.7"
  }
}