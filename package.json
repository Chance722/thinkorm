{
  "name": "thinkorm",
  "version": "1.0.0",
  "description": "THINKNODE ORM.",
  "scripts": {
    "test": "npm run eslint && npm run test-cov",
    "test-cov": "istanbul cover ./node_modules/mocha/bin/_mocha -- -t 50000 --recursive  -R spec test/",
    "compile": "babel src -d lib",
    "watch-compile": "npm run compile -- --watch",
    "prepublish": "npm run compile && npm test",
    "eslint": "eslint src/",
    "pack": "npm shrinkwrap && shrinkpack ."
  },
  "babel": {
    "presets": [
      "es2015-loose",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richenlin/thinknode.git"
  },
  "keywords": [
    "thinkorm",
    "thinknode",
    "framework",
    "orm"
  ],
  "engines": {
    "node": ">4.0.0"
  },
  "author": {
    "name": "lihao",
    "email": "lihao19860813@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/richenlin/thinkorm/issues"
  },
  "homepage": "https://github.com/richenlin/thinkorm",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-2": "^6.11.0",
    "eslint": "3.1.1",
    "istanbul": "0.4.4",
    "mocha": "2.5.3",
    "muk": "0.5.2"
  },
  "dependencies": {
    "babel-runtime": "6.9.2",
    "bluebird": "3.4.1",
    "ejs": "2.4.2",
    "knex": "^0.11.9",
    "mime": "1.3.4",
    "multiparty": "4.1.2",
    "mysql": "^2.11.1",
    "mysql2": "^1.0.0-rc.8"
  },
  "maintainers": [
    {
      "name": "richenlin",
      "email": "richenlin@gmail.com"
    },
    {
      "name": "lsliangshan",
      "email": "loveliangshan@126.com"
    },
    {
      "name": "richerdlee",
      "email": "richerdlee@163.com"
    }
  ]
}
