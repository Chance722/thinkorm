{
  "name": "thinkorm",
  "version": "0.4.4",
  "description": "A flexible, lightweight and powerful Object-Relational Mapper for Node.js.",
  "main": "index.js",
  "scripts": {
    "test": "npm run test-cov",
    "test-cov": "istanbul cover ./node_modules/mocha/bin/_mocha -- -t 50000 --recursive  -R spec test/",
    "compile": "babel src -d lib",
    "compile-test": "babel exmple/model/src -d exmple/model/lib",
    "watch-compile": "npm run compile -- --watch",
    "prepublish": "npm run compile && npm test",
    "eslint": "eslint src/",
    "pack": "npm shrinkwrap && shrinkpack ."
  },
  "babel": {
    "presets": [
      "es2015-loose",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richenlin/thinkorm.git"
  },
  "author": "richenlin",
  "license": "MIT",
  "keywords": [
    "ORM",
    "thinkorm"
  ],
  "bugs": {
    "url": "https://github.com/richenlin/thinkorm/issues"
  },
  "homepage": "https://github.com/richenlin/thinkorm#readme",
  "engines": {
    "node": "4.x.x"
  },
  "devDependencies": {
    "async": "^2.1.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-stage-2": "^6.18.0",
    "eslint": "^3.8.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "mongodb": "^2.2.11",
    "muk": "^0.5.2",
    "pg": "^6.1.0"
  },
  "dependencies": {
    "babel-runtime": "6.18.0",
    "bluebird": "3.4.6",
    "co": "4.6.0",
    "knex": "0.12.6",
    "mysql": "2.11.1"
  },
  "maintainers": [
    {
      "name": "richenlin",
      "email": "richenlin@gmail.com"
    },
    {
      "name": "richerdlee",
      "email": "richerdlee@163.com"
    }
  ]
}
