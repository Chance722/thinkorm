{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "strict": [0],
    "eqeqeq": 1,
    "quotes": [1, "single", {"allowTemplateLiterals": true}],
    "no-underscore-dangle": 0,
    "eol-last": 0,
    "camelcase": 1,
    "no-loop-func": 0,
    "no-trailing-spaces": 0,
    "consistent-return": 0,
    "new-cap": 0,
    "no-shadow": 0,
    "semi": 0,
    "no-process-exit": 0,
    "no-empty": 0,
    "yoda": 0,
    "no-const-assign": 1,
    "no-new-func": 0,
    "no-labels": 0,
    "no-this-before-super": 1, 
    "no-undef": 1,  
    "no-unreachable": 1, 
    "no-unused-vars": 1, 
    "valid-typeof": 1
  }
}